{
  "required_api_version": "^2.0.0",
  "name": "FD search",
  "description": "An extension to Search for files and directories.",
  "developer_name": "Anish",
  "icon": "images/icon.png",
    "options": {
        "query_debounce": 0.5
    },
  "preferences": [
    {
      "id": "filesearch",
      "type": "keyword",
